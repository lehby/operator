<view class="gastotal">
  <view class="gastotal_top">
    <image src="../../imgs/gas.png" />
    <view>订气订单数量</view>
  </view>
  <view class="gastotal_bottom">
    <view class="gastotal_bottom_left">
      <view class="phoneOrder">
        <image src="../../imgs/Answer.png" />
        <view>打电话订气订单</view>
      </view>
      <view class="phoneOrder_num">111,111,00</view>
    </view>
    <view class="gastotal_bottom_right">
      <view class="phoneOrder">
        <image src="../../imgs/system.png" />
        <view>系统订气订单</view>
      </view>
      <view class="phoneOrder_num">111,111,00</view>
    </view>
  </view>
</view>
<view class="gastotal">
  <view class="gastotal_top">
    <image src="../../imgs/Repair.png" />
    <view>维修订单数量</view>
  </view>
  <view class="gastotal_bottom">
    <view class="gastotal_bottom_left">
      <view class="phoneOrder">
        <image src="../../imgs/Answer.png" />
        <view>打电话维修订单</view>
      </view>
      <view class="phoneOrder_num">111,111,00</view>
    </view>
    <view class="gastotal_bottom_right">
      <view class="phoneOrder">
        <image src="../../imgs/system.png" />
        <view>系统维修订单</view>
      </view>
      <view class="phoneOrder_num">111,111,00</view>
    </view>
  </view>
</view>


