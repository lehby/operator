<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_input">
    <image class='img_m' src="../../imgs/calendar.png" />
    <view class="weui-cell__hd">
      <view class="weui-label">选择起始日期</view>
    </view>
    <view class="weui-cell__bd">
      <picker mode="date" value="{{StartDate}}" start="2017-09-01" end="2030-09-01" bindchange="bindDateChange">
        <view class="weui-input" style="padding-left:276rpx;">{{StartDate}}</view>
      </picker>
    </view>
  </view>
</view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_input">
    <image class='img_m' src="../../imgs/calendar.png" />
    <view class="weui-cell__hd">
      <view class="weui-label">选择结束日期</view>
    </view>
    <view class="weui-cell__bd">
      <picker mode="date" value="{{EndDate}}" start="2017-09-01" end="2030-09-01" bindchange="onDateChange">
        <view class="weui-input" style="padding-left:276rpx;">{{EndDate}}</view>
      </picker>
    </view>
  </view>
</view>
